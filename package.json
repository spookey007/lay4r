{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run frontend\" \"npm run start:websocket\"",
    "frontend": "next dev",
    "websockenpmt": "nodemon server/index.js",
    "build": "prisma generate && next build",
    "build:turbo": "prisma generate && next build --turbopack",
    "postinstall": "prisma generate",
    "prebuild": "prisma generate",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:websocket": "cross-env NODE_ENV=development node server/server.js",
    "start:frontend": "next start",
    "start:both": "concurrently \"next start\" \"npm run start\"",
    "dev:both": "concurrently \"npm run frontend\" \"npm run websocket\"",
    "start:prod:websocket": "cross-env NODE_ENV=production node server/index.js",
    "start:prod:frontend": "cross-env NODE_ENV=production next start",
    "start:prod:both": "concurrently \"npm run start:prod:frontend\" \"npm run start:prod:websocket\"",
    "start:prod": "npm run build && concurrently \"npm run start:prod:frontend\" \"npm run start:prod:websocket\"",
    "start:prod:no-build": "concurrently \"npm run start:prod:frontend\" \"npm run start:prod:websocket\"",
    "lint": "eslint",
    "set-admin": "node server/scripts/set-first-admin.js",
    "seed": "node prisma/seed.js",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed"
  },
